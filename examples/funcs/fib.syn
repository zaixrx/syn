// syn is statically typed
func fib(n: Int) -> Int {
    if n <= 1 {
        return n;
    }
    let a = 0;
    let b = 1;
    while n > 0 {
        a = a + b;
	b = a - b;
        n -= 1;
    }
    return a; 
} 

// functions are first class
func get_fib() -> Func { 
    return fib; 
}

func main() {
    let x = 30;
    println("get_fib()({}) = {}", x, get_fib()(x));
}

