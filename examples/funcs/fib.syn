// syn is statically typed
func fib(n: Float) -> Float {
    if n <= 1.0 {
        return 1.0;
    }
    let a = 1.0;
    let b = 1.0;
    while n > 0.0 {
        a = a + b;
        b = a - b;
        n = n - 1.0;
    }
    return a; 
} 

// functions are first class
func get_fib() -> Func { 
    return fib; 
}

func main() {
    let x = 123.0;
    println("get_fib()({}) = {}", x, get_fib()(x));
}

