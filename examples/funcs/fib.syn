func fib(n: Float) -> Float {
	if n <= 1 {
		return 1;
	}
 	let a = 1.0;
 	let b = 1.0;
 	while n > 0 {
 		a = a + b;
 		b = a - b;
		n = n - 1;
 	}
 	return a;
}

func get_fib() -> Func {
    return fib;
}

func main() {
    let target = 123;
	print("get_fib()(", target, ") =", get_fib()(target));
}
